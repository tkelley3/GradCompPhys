Cell["\<\
Let\[CloseCurlyQuote]s consider a spin-1 system. These systems can be \
represented as vectors with 3 components. We can construct the rotation \
matrix for this states using the Pauli matrices. Then, use this matrix to \
rotate an \[OpenCurlyDoubleQuote]up\[CloseCurlyDoubleQuote] state. 
First some set up,\
\>", "Text",
 CellChangeTimes->{{3.733580995001109*^9, 3.7335810221180973`*^9}, {
  3.7511255066644897`*^9, 3.751125510376584*^9}, {3.7515638258650074`*^9, 
  3.7515638871521854`*^9}, {3.7515639376761312`*^9, 
  3.751564006476965*^9}},ExpressionUUID->"4b0fa0f7-6b43-49fc-becc-\
5bd5b0312638"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Id", " ", "=", " ", 
   RowBox[{"IdentityMatrix", "[", "3", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"state0", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{"1", ",", "0", ",", "0"}], "}"}]}], ";", " ", 
  RowBox[{"(*", 
   RowBox[{"represents", " ", "state", " ", 
    TemplateBox[{RowBox[{"1", ",", "1"}]},
     "Ket"]}], "*)"}], "\n", 
  RowBox[{"Sx", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", 
       FractionBox["h", 
        SqrtBox["2"]], ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       FractionBox["h", 
        SqrtBox["2"]], ",", "0", ",", 
       FractionBox["h", 
        SqrtBox["2"]]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       FractionBox["h", 
        SqrtBox["2"]], ",", "0"}], "}"}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Sy", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", 
       FractionBox[
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "h"}], 
        SqrtBox["2"]], ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       FractionBox[
        RowBox[{"\[ImaginaryI]", " ", "h"}], 
        SqrtBox["2"]], ",", "0", ",", 
       FractionBox[
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "h"}], 
        SqrtBox["2"]]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       FractionBox[
        RowBox[{"\[ImaginaryI]", " ", "h"}], 
        SqrtBox["2"]], ",", "0"}], "}"}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Sz", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"h", ",", "0", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0", ",", 
       RowBox[{"-", "h"}]}], "}"}]}], "}"}]}], ";"}]}], "Code",
 CellChangeTimes->{{3.733578835766776*^9, 3.7335788371241508`*^9}, {
  3.733579332659305*^9, 3.7335793405790744`*^9}, {3.7335795068470125`*^9, 
