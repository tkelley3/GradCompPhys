Cell[BoxData[{
 RowBox[{
  RowBox[{"R", " ", "=", " ", 
   RowBox[{"Id", " ", "-", " ", 
    RowBox[{"\[ImaginaryI]", " ", 
     FractionBox["Sx", "h"], "\[Theta]"}], " ", "+", " ", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "!"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        FractionBox["\[Theta]", "h"]}], ")"}], "2"], 
     RowBox[{"Sx", ".", "Sx"}]}], " ", "+", " ", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"3", "!"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        FractionBox["\[Theta]", "h"]}], ")"}], "3"], 
     RowBox[{"Sx", ".", "Sx", ".", "Sx"}]}], "+", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"4", "!"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        FractionBox["\[Theta]", "h"]}], ")"}], "4"], 
     RowBox[{"Sx", ".", "Sx", ".", "Sx", ".", "Sx"}]}]}]}], " "}], "\n", 
 RowBox[{"R", "//", "MatrixForm"}]}], "Code",
 CellChangeTimes->{{3.733578835766776*^9, 3.7335788371241508`*^9}, {
