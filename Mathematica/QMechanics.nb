Cell[CellGroupData[{

Cell["Wave Functions", "Section",
 CellChangeTimes->{{3.7335824033930397`*^9, 3.7335824052541513`*^9}, {
  3.751564584984939*^9, 3.751564616130895*^9}, {3.7515647041824408`*^9, 
  3.7515647181249633`*^9}},ExpressionUUID->"05fbfab1-2d73-40c3-aff6-\
1c8aa5b6b117"],

Cell["\<\
Consider a 1D wavefunction as defined below. Let\[CloseCurlyQuote]s graph it \
and define some integrals to calculate expectation values.\
\>", "Text",
 CellChangeTimes->{{3.7335824033930397`*^9, 3.7335824052541513`*^9}, {
  3.751564584984939*^9, 3.751564616130895*^9}, {3.7515647041824408`*^9, 
  3.751564819312437*^9}},ExpressionUUID->"821588a5-7801-4df7-a57e-\
d10fef697aba"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Psi]3", "[", "x_", "]"}], " ", ":=", 
   RowBox[{
    SqrtBox[
     FractionBox["8", 
      RowBox[{"5", "a"}]]], " ", 
    SuperscriptBox[
     RowBox[{"Cos", "[", 
      RowBox[{"\[Pi]", " ", 
       FractionBox["x", 
        RowBox[{"2", "a"}]]}], "]"}], "3"]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"\[Psi]3", "[", "x", "]"}], "2"], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "a"}], ",", "a"}], "}"}]}], " ", "]"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"dx", " ", "=", " ", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"x", "^", "2"}], " ", 
      SuperscriptBox[
       RowBox[{"\[Psi]3", "[", "x", "]"}], "2"]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "a"}], ",", "a"}], "}"}]}], " ", "]"}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"dp", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["h", "2"]}], " ", 
      RowBox[{"\[Psi]3", "[", "x", "]"}], " ", 
      RowBox[{
       RowBox[{"\[Psi]3", "''"}], "[", "x", "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "a"}], ",", "a"}], "}"}]}], " ", "]"}]}], "\n"}], "\n", 
 RowBox[{"uncert", " ", "=", " ", 
  RowBox[{
   RowBox[{
    SqrtBox["dp"], "*", 
    SqrtBox["dx"]}], "//", "PowerExpand"}]}], "\n", 
 RowBox[{"uncert", "//", "N"}]}], "Code",
 CellChangeTimes->{
  3.733582502741629*^9, {3.7335825336465015`*^9, 3.733582542503952*^9}, {
   3.7515646524625397`*^9, 3.7515646863013926`*^9}, {3.7515648245873632`*^9, 
   3.751564847058283*^9}},
 CellLabel->
  "In[100]:=",ExpressionUUID->"48f60021-e645-4229-8f75-e1f46fbf62ff"],

Cell["\<\
We can even get fancy and work with the wave functions for the hydrogen atom. \
\>", "Text",
 CellChangeTimes->{{3.7335825615137806`*^9, 3.73358256314091*^9}, {
  3.7515648623274255`*^9, 
  3.7515648903694696`*^9}},ExpressionUUID->"ebdac930-2b00-4331-a636-\
fcbb663bb905"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"R", "[", 
   RowBox[{"n_", ",", "l_", ",", "r_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Sqrt", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "-", "l", "-", "1"}], ")"}], "!"}], "/", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "+", "l"}], ")"}], "!"}]}], "]"}], " ", 
   RowBox[{"E", "^", 
    RowBox[{"(", 
     RowBox[{"-", 
      RowBox[{"(", 
       RowBox[{"r", "/", "n"}], ")"}]}], ")"}]}], " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"2", " ", "r"}], ")"}], "/", "n"}], ")"}], "^", "l"}], " ", 
   RowBox[{"2", "/", 
    RowBox[{"n", "^", "2"}]}], " ", 
   RowBox[{"LaguerreL", "[", 
    RowBox[{
     RowBox[{"n", "-", "l", "-", "1"}], ",", 
     RowBox[{
      RowBox[{"2", " ", "l"}], "+", "1"}], ",", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"2", " ", "r"}], ")"}], "/", "n"}]}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Y", "[", 
    RowBox[{"l_", ",", "m_", ",", "\[Theta]_", ",", "\[Phi]_"}], "]"}], ":=", 
   RowBox[{"SphericalHarmonicY", "[", 
    RowBox[{"l", ",", "m", ",", "\[Theta]", ",", "\[Phi]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Psi]", "[", 
   RowBox[{
   "n_", ",", "l_", ",", "m_", ",", "r_", ",", "\[Theta]_", ",", "\[Phi]_"}], 
   "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"R", "[", 
    RowBox[{"n", ",", "l", ",", "r"}], "]"}], "*", 
   RowBox[{"Y", "[", 
    RowBox[{"l", ",", "m", ",", "\[Theta]", ",", "\[Phi]"}], 
    "]"}]}]}]}], "Code",
 CellLabel->
  "In[113]:=",ExpressionUUID->"c0bfe114-e27b-4c93-8dd8-7f95abba3b25"],

Cell["\<\
The uncertainty in the position of the electron is found by calculating some \
expectation values. Below, I have made up some kind of radial wave function \
from the superposition of radial wave functions\
\>", "Text",
 CellChangeTimes->{{3.7335826950118685`*^9, 3.733582729839178*^9}, {
  3.733582945532256*^9, 3.7335829835555716`*^9}, {3.73358316752059*^9, 
  3.733583177568657*^9}, {3.73358324476429*^9, 3.73358328204564*^9}, {
  3.751565100633092*^9, 
  3.7515651422079144`*^9}},ExpressionUUID->"0a4efbae-ea5b-4d29-9011-\
d22252dc9ed6"],

Cell[BoxData[{
 RowBox[{"rexp", " ", "=", " ", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["r", "3"], 
      RowBox[{"19", " ", 
       SuperscriptBox["a", "3"]}]], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"16", "*", 
        SuperscriptBox[
         RowBox[{"R", "[", 
          RowBox[{"1", ",", "0", ",", 
           FractionBox["r", "a"]}], "]"}], "2"]}], "+", 
       SuperscriptBox[
        RowBox[{"R", "[", 
         RowBox[{"2", ",", "1", ",", 
          FractionBox["r", "a"]}], "]"}], "2"], "+", 
       RowBox[{"2", "*", 
        SuperscriptBox[
         RowBox[{"R", "[", 
          RowBox[{"2", ",", "1", ",", 
           FractionBox["r", "a"]}], "]"}], "2"]}]}], ")"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"r", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}]}], "\n", 
 RowBox[{"rsexp", " ", "=", " ", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
